<div class="show_order_items">
  <tr class="show_order_items">
    <td><span class="nn">{{{n}}}</span></td>
    <td class="show_product_image" style="{{{show_product_image_hc}}}"><div style="{{{show_product_image_dn}}}">{{{img}}}</div></td>
    <td class="show_product_sku" style="{{{show_product_sku_hc}}}"><div style="{{{show_product_sku_dn}}}">{{{sku}}}</div></td>
    <td colspan="4"><span class="title">{{{title}}}<i class="show_product_purchase_note" style="{{{show_product_purchase_note_dn}}}"><br><div style="{{{show_product_purchase_note_dn}}}">{{{description}}}</div></i></span></td>
    <td><span class="qty">{{{qty}}}</span></td>
    <td class="show_product_weight" style="{{{show_product_weight_hc}}}"><div style="{{{show_product_weight_dn}}}">{{{weight}}}<br>{{{total_weight}}}</div></td>
    <td class="show_product_dimensions" style="{{{show_product_dimensions_hc}}}"><div style="{{{show_product_dimensions_dn}}}">{{{dimension}}}</div></td>
    <td><span class="base_price">{{{base_price}}}</span></td>
    <td><span class="discount">{{{discount}}}</span></td>
    <td><span class="tax">{{{tax}}}</span></td>
    <td><span class="nettotal">{{{nettotal}}}</span></td>
  </tr>
</div>
