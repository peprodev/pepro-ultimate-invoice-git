<div class="show_order_items" class="{{{extra_classes}}}">
  <tr class="show_order_items">
    <td><span class="nn">{{{n}}}</span></td>
    <td class="show_product_image" style="{{{show_product_image_hc}}}"><div style="{{{show_product_image_dn}}}">{{{img}}}</div></td>
    <td class="show_product_sku" style="{{{show_product_sku_hc}}}"><div style="{{{show_product_sku_dn}}}">{{{sku}}}</div></td>
    <td colspan="{{{product_description_colspan}}}"><span class="title">{{{title}}}<i class="show_product_purchase_note" style="{{{show_product_purchase_note_dn}}}"><br><div style="{{{show_product_purchase_note_dn}}}">{{{description}}}</div></i></span></td>
    <td><span class="qty">{{{qty}}}</span></td>
    <td class="show_product_weight" style="{{{show_product_weight_hc}}}"><div style="{{{show_product_weight_dn}}}"><span class='single_weight'>{{{weight}}}</span><span class='totalweight'><br>{{{total_weight}}}</span></div></td>
    <td class="show_product_dimensions" style="{{{show_product_dimensions_hc}}}"><div style="{{{show_product_dimensions_dn}}}">{{{dimension}}}</div></td>
    <td><span class="base_price">{{{base_price}}}</span></td>
    <td><span class="show_discount_precent">{{{discount}}}</span></td>
    <td><span class="show_product_tax">{{{tax}}}</span></td>
    <td colspan="{{{product_nettotal_colspan}}}"><span class="nettotal">{{{nettotal}}}</span></td>
  </tr>
</div>
