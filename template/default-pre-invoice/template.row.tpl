<tr if="show_order_items">
  <td><span class="nn">{{{n}}}</span></td>
  <td class="show_product_image">{{{img}}}</td>
  <td class="show_product_sku"><span class="sku">{{{sku}}}</span></td>
  <td colspan="4"><span class="title">{{{title}}}<i class="show_product_purchase_note">{{{description}}}</i></span></td>
  <td><span class="qty">{{{qty}}}</span></td>
  <td class="show_product_weight"><span class="weight">{{{weight}}}<br>{{{total_weight}}}</span></td>
  <td class="show_product_dimensions"><span class="dimension">{{{dimension}}}</span></td>
  <td><span class="base_price">{{{base_price}}}</span></td>
  <td><span class="discount">{{{discount}}}</span></td>
  <td><span class="tax">{{{tax}}}</span></td>
  <td><span class="nettotal">{{{nettotal}}}</span></td>
</tr>
