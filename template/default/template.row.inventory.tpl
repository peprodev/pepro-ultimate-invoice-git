<tr if="show_order_items">
  <td>{{{n}}}</td>
  <td if="show_product_image_inventory">{{{img}}}</td>
  <td if="show_product_sku_inventory">{{{sku}}}</td>
  <td if="show_shelf_number_id">{{{shelf_number_id}}}</td>
  <td colspan="2">{{{title}}}</td>
  <td if="show_inventory_price">{{{base_price}}}</td>
  <td if="show_product_weight_in_inventory" width="80px">{{{weight}}}</td>
  <td if="show_product_total_weight_in_inventory" width="80px">{{{total_weight}}}</td>
  <td if="show_product_dimensions_in_inventory" width="120px">{{{dimension}}}</td>
  <td if="show_product_quantity_in_inventory" >{{{qty}}}</td>
  <td if="show_product_note_in_inventory" colspan="2">{{{description}}}</span></td>
</tr>
